<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="2" name="SauceDemo Smoke Test" parallel="tests">
    <listeners>
        <listener class-name="tests.TestListener"/>
        <listener class-name="tests.AnnotationTransformer"/>
    </listeners>
    <test name="Thread #1">
        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.AddToCartTest"/>
            <class name="tests.BaseTest"/>
        </classes>
    </test> <!-- Test -->
    <test name="Thread #2">
        <classes>
            <class name="tests.CartTest"/>
            <class name="tests.LocatorTest"/>
        </classes>
    </test> <!-- Test -->
</suite>
